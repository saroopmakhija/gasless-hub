  "engines": {
    "bun": ">=1.3.3",
    "node": ">=24"
  },
  "name": "samui",
  "packageManager": "bun@1.3.3",
  "pnpm": {
    "onlyBuiltDependencies": ["@tailwindcss/oxide", "esbuild", "lefthook", "sharp"]
  },
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "clean": "find . -type d \\( -name 'node_modules' -o -name 'build' -o -name 'dist' -o -name '.turbo' -o -name '.wxt' -o -name '.expo' -o -name 'android' -o -name 'ios' -o -name '.astro' -o -name '.wrangler' \\) -prune -exec rm -rf '{}' +",
    "dev": "turbo run dev",
    "lint": "biome check --error-on-warnings",
    "lint:fix": "biome check --error-on-warnings --write",
    "preinstall": "npx only-allow bun",
    "prepare": "lefthook install",
    "spell-check": "cspell -c ./cspell.config.mts .",
    "test": "vitest run",
    "test:integration": "vitest run --config=vitest.integration.config.ts",
    "test:integration:watch": "vitest --watch --config=vitest.integration.config.ts",
    "test:watch": "vitest --watch"
  },
  "type": "module",
  "version": "0.0.0",
  "workspaces": ["apps/*", "packages/*", "turbo/*"]
}
